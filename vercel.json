

{
"functions": {
"api/**/*.php": {
"runtime": "vercel-php@0.6.0"
}
},
"routes": [
{ "src": "/(.*)", "dest": "/api/index.php" },
{ "src": "/api/static/assets/css/style.css", "dest": "/api/static/assets/css/style.css" },
{ "src": "/api/static/assets/js/script.js", "dest": "/api/static/assets/js/script.js" }]
}
